							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CriticalChance" />
							<Label id="CriticalChanceLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="OffensiveElements" class="BoxedAttributes" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_ElementsDamage', '#DOTA_Tooltip_ElementsDamage_Description')" onmouseout="DOTAHideTitleTextTooltip()"/>
						</Panel>
						<Panel id="Page2" class="HeroStatsPage">
						<Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_DefensiveStats" />
						<Panel id="PhysArmor" class="InventoryAttributeWithValue">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Armor" />
							<Label id="PhysArmorLabel" class="WindowText InventoryAttributeValue" text="0 (0%)" />
						</Panel>
						<Panel id="HealthRegen" class="InventoryAttributeWithValue">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_HealthRegen" />
							<Label id="HealthRegenLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="PhysBlock" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_Block', '#DOTA_Tooltip_Block_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Block"/>
							<Label id="PhysBlockLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="MagicBlock" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_MagicBlock', '#DOTA_Tooltip_MagicBlock_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_MagicBlock" />
							<Label id="MagicBlockLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="DamageReduction" class="InventoryAttributeWithValue"  onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_DamageReduction', '#DOTA_Tooltip_DamageReduction_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_DamageReduction" />
							<Label id="DamageReductionLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="BuffAmplification" class="InventoryAttributeWithValue"  onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_BuffAmplification', '#DOTA_Tooltip_BuffAmplification_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Panel id="StrengthIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_BuffAmplification" />
							<Label id="BuffAmplificationLabel" class="WindowText InventoryAttributeValue" text="100%" />
						</Panel>
						<Panel id="DefensiveElements" class="BoxedAttributes" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_ElementsProtection', '#DOTA_Tooltip_ElementsProtection_Description')" onmouseout="DOTAHideTitleTextTooltip()"/>
						</Panel>
					</Panel>
				</Panel>
				<Panel id="InventoryContainer" class="InventoryContainer"/>
			</Panel>
			<Image class="WindowCloseIcon" src="file://{images}/custom_game/hud/close_button.png" scaling="stretch-to-fit-preserve-aspect" onactivate="CloseWindow()"/>
		</Panel>
		<Panel id="ItemTooltip" style="visibility: collapse;" hittest="false" hittestchildren="false">
			<Panel class="ItemTooltipHeader">
				<Panel>
					<DOTAItemImage id="ItemTooltipImage" class="ItemTooltipImage" showtooltip="false" hittest="false" scaling="stretch-to-fit-preserve-aspect" itemname="item_third_eye"/>
				</Panel>
				<Panel class="ItemTooltipName">
					<Label id="ItemTooltipNameLabel" html="true" class="ItemTooltipNameLabel" text="Item name" />
				</Panel>
			</Panel>
			<Panel class="ItemTooltipDescription">
				<Panel class="ItemTooltipInfoContainer">
					<Label id="ItemTooltipRarityLabel" html="true" class="ItemTooltipRarityLabel" text="Item rarity" />
					<Label id="ItemTooltipTypeLabel" html="true" class="ItemTooltipTypeLabel"  text="Item type" />
				</Panel>
				<Label id="ItemTooltipQualityLabel" class="ItemTooltipQualityLabel" html="true" text="Quality: 0%" />
				<Label id="ItemTooltipLabel" class="ItemTooltipLabel" html="true" text="Item description" />
				<Panel id="ItemTooltipStatsContainer" class="ItemTooltipStatsContainer" />
			</Panel>
		</Panel>
		<Panel id="ConflictDialogPanel">
			<Panel class="OldNewLabelsContainer">
				<Label text="#DOTA_Inventory_new_item" class="NewLabel" />
				<Label text="#DOTA_Inventory_current_item" class="OldLabel" />
			</Panel>
			<Panel class="ItemPanel">
				<Panel class="Item">
					<Panel class="ItemTooltipHeader">
						<Panel>
							<DOTAItemImage id="ItemTooltipImage1" class="ItemTooltipImage" showtooltip="false" hittest="false" scaling="stretch-to-fit-preserve-aspect" itemname="item_third_eye"/>
						</Panel>
						<Panel class="ItemTooltipName">
							<Label id="ItemTooltipNameLabel1" html="true" class="ItemTooltipNameLabel" text="Item name" />
						</Panel>
					</Panel>
					<Panel class="ItemTooltipDescription">
						<Panel class="ItemTooltipInfoContainer">
							<Label id="ItemTooltipRarityLabel1" html="true" class="ItemTooltipRarityLabel" text="Item rarity" />
							<Label id="ItemTooltipTypeLabel1" html="true" class="ItemTooltipTypeLabel" text="Item type" />
						</Panel>
						<Label id="ItemTooltipQualityLabel1" class="ItemTooltipQualityLabel" html="true" text="Quality: 0%" />
						<Label id="ItemTooltipLabel1" class="ItemTooltipLabel" html="true" text="Item description" />
						<Panel id="ItemTooltipStatsContainer1" class="ItemTooltipStatsContainer" />
					</Panel>
				</Panel>
				<Panel class="Item">
					<Panel class="ItemTooltipHeader">
						<Panel>
							<DOTAItemImage id="ItemTooltipImage2" class="ItemTooltipImage" showtooltip="false" hittest="false" scaling="stretch-to-fit-preserve-aspect" itemname="item_third_eye"/>
						</Panel>
						<Panel class="ItemTooltipName">
							<Label id="ItemTooltipNameLabel2" html="true" class="ItemTooltipNameLabel" text="Item name" />
						</Panel>
					</Panel>
					<Panel class="ItemTooltipDescription">
						<Panel class="ItemTooltipInfoContainer">
							<Label id="ItemTooltipRarityLabel2" html="true" class="ItemTooltipRarityLabel" text="Item rarity" />
							<Label id="ItemTooltipTypeLabel2" html="true" class="ItemTooltipTypeLabel" text="Item type" />
						</Panel>
						<Label id="ItemTooltipQualityLabel2" class="ItemTooltipQualityLabel" html="true" text="Quality: 0%" />
						<Label id="ItemTooltipLabel2" class="ItemTooltipLabel" html="true" text="Item description" />
						<Panel id="ItemTooltipStatsContainer2" class="ItemTooltipStatsContainer" />
					</Panel>
				</Panel>
			</Panel>
			<Panel styte="flow-children: right; width: 100%; height: 150px;">
				<Panel class="InventoryButton Normal" style="margin-left: 100px; margin-bottom: 30px;" onactivate="OnInventoryChangeEquippedItemClicked()">
					<Label text ="#DOTA_Inventory_btn_replace" />
				</Panel>
				<Panel class="InventoryButton Normal" style="margin-left: 400px; margin-bottom: 30px;" onactivate="OnInventoryKeepBothClicked()">
					<Label text ="#DOTA_Inventory_btn_keep_both" />
				</Panel>
			</Panel>
		</Panel>
		<!-- weird hack to make them auto recompile -->
		<Panel style="visibility: collapse;" >
		<Image src="file://{images}/custom_game/hud/fire_element.png" />
		<Image src="file://{images}/custom_game/hud/inferno_element.png" />
		<Image src="file://{images}/custom_game/hud/frost_element.png" />
		<Image src="file://{images}/custom_game/hud/earth_element.png" />
		<Image src="file://{images}/custom_game/hud/nature_element.png" />
		<Image src="file://{images}/custom_game/hud/holy_element.png" />
		<Image src="file://{images}/custom_game/hud/void_element.png" />
		</Panel>
	</Panel>
</root>