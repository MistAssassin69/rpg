									<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage9" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_ring_of_protection"/>
									<Label text="#DOTA_Inventory_slot_ring"/>
								</Panel>
							</Panel>
							<Panel id="InventoryEquippedSlot10" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(10)" onmouseover="ShowEquippedItemTooltip(10)" onmouseout="HideItemTooltip()" showtooltip="false">
								<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage10" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_belt_of_strength"/>
								<Label text="#DOTA_Inventory_slot_belt"/>
							</Panel>
							<Panel class="RightRings">
								<Panel id="InventoryEquippedSlot11" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(11)" onmouseover="ShowEquippedItemTooltip(11)" onmouseout="HideItemTooltip()" showtooltip="false">
									<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage11" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_shadow_amulet"/>
									<Label text="#DOTA_Inventory_slot_amulet"/>
								</Panel>
							</Panel>
						</Panel>
					</Panel>
					<Panel class="HeroStats" style="padding-top: 30px;">
						<Panel class="SwitchPageButtons">
							<Panel id="Page0Button" class="InventoryButton" onactivate="OnSwitchPageButtonClick(0)">
								<Label text ="I" />
							</Panel>
							<Panel id="Page1Button" class="InventoryButton" onactivate="OnSwitchPageButtonClick(1)">
								<Label text ="II" />
							</Panel>
							<Panel id="Page2Button" class="InventoryButton" onactivate="OnSwitchPageButtonClick(2)">
								<Label text ="III" />
							</Panel>
						</Panel>
						<Panel class="HeroNameContainer">
							<DOTAHeroImage id="HeroIcon" class="HeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="icon" />
							<DOTAUserName id="HeroName" class="WindowText HeroName" steamid="local" />
						</Panel>
						<Panel id="Page0" class="HeroStatsPage">
						<Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_Attributes" />
						<Panel class="InventoryAttributeWithValue">
							<Panel id="StrengthIcon" class="StrengthIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" style="margin-left: 0px; margin-bottom: 0px;" text="#DOTA_Tooltip_Strength" />
							<Label id="StrengthLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel class="InventoryAttributeWithValue">
							<Panel id="AgilityIcon" class="AgilityIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" style="margin-left: 0px; margin-bottom: 0px;" text="#DOTA_Tooltip_Agility" />
							<Label id="AgilityLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel class="InventoryAttributeWithValue">
							<Panel id="IntelligenceIcon" class="IntelligenceIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" style="margin-left: 0px; margin-bottom: 0px;" text="#DOTA_Tooltip_Intelligence" />
							<Label id="IntelligenceLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_GeneralStats" />
						<Panel class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/health.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Health" />
							<Label id="HealthLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="Mana" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/mana.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Mana" />
							<Label id="ManaLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="CooldownReduction" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/mana.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CooldownReduction" />
							<Label id="CooldownReductionLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="Level" class="InventoryAttributeWithValue">
							<Panel id="IntelligenceIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_HeroLevel" />
							<Label id="LevelLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="CurrentXP" class="InventoryAttributeWithValue">
							<Panel id="IntelligenceIcon" class="StatIcon" hittest="false" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CurrentXP" />
							<Label id="CurrentXPLabel" class="WindowText InventoryAttributeValue" text="0/0" />
						</Panel>
						</Panel>
						<Panel id="Page1" class="HeroStatsPage">
						<Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_OffensiveStats" />
						<Panel id="SpellDamage" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_SpellDamage', '#DOTA_Tooltip_SpellDamage_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/spelldamage.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_SpellDamage" />
							<Label id="SpellDamageLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="Spellhaste" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_SpellHaste', '#DOTA_Tooltip_SpellHaste_Description')" onmouseout="DOTAHideTitleTextTooltip()">>
							<Image class="StatIcon" src="file://{images}/custom_game/hud/spellhaste.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_SpellHaste" />
							<Label id="SpellhasteLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="AttackRange" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="s2r://panorama/images/hud/reborn/icon_attack_range_psd.vtex" style="wash-color: grey;"/>
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_AttackRange" />
							<Label id="AttackRangeLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="AttackSpeed" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="s2r://panorama/images/hud/reborn/icon_attack_speed2_psd.vtex" />
