							</Panel>
							<Panel id="InventoryEquippedSlot7" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(7)" onmouseover="ShowEquippedItemTooltip(7)" onmouseout="HideItemTooltip()" showtooltip="false">
								<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage7" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_shivas_guard"/>
								<Label text="#DOTA_Inventory_slot_shoulder"/>
							</Panel>
							<Panel id="InventoryEquippedSlot8" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(8)" onmouseover="ShowEquippedItemTooltip(8)" onmouseout="HideItemTooltip()" showtooltip="false">
								<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage8" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_gloves"/>
								<Label text="#DOTA_Inventory_slot_gloves"/>
							</Panel>
						</Panel>
						<Panel id="MiddleItemSlots" class="MiddleItemSlots">
							<Panel id="InventoryEquippedSlot4" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(4)" onmouseover="ShowEquippedItemTooltip(4)" onmouseout="HideItemTooltip()" showtooltip="false">
								<DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage4" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_helm_of_iron_will"/>
								<Label text="#DOTA_Inventory_slot_helmet"/>
							</Panel>
						</Panel>
						<Panel id="MiddleItemSlotsFromBottom" class="MiddleItemSlots FromBottom">
