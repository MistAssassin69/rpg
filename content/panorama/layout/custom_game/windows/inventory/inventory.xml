							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_BaseAttackTime" />
							<Label id="BaseAttackTimeLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="Attack Damage" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="s2r://panorama/images/hud/reborn/icon_damage_psd.vtex" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_AttackDamage" />
							<Label id="AttackDamageLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="MoveSpeed" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="s2r://panorama/images/hud/reborn/icon_speed_psd.vtex" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_MoveSpeed" />
							<Label id="MoveSpeedLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="ManaRegen" class="InventoryAttributeWithValue">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/manaregen.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_ManaRegen" />
							<Label id="ManaRegenLabel" class="WindowText InventoryAttributeValue" text="0" />
						</Panel>
						<Panel id="DebuffAmplification" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_DebuffAmplification', '#DOTA_Tooltip_DebuffAmplification_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/manaregen.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_DebuffAmplification" />
							<Label id="DebuffAmplificationLabel" class="WindowText InventoryAttributeValue" text="100%" />
						</Panel>
						<Panel id="DebuffResistance" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_DebuffResistance', '#DOTA_Tooltip_DebuffResistance_Description')" onmouseout="DOTAHideTitleTextTooltip()">
							<Image class="StatIcon" src="file://{images}/custom_game/hud/manaregen.png" />
							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_DebuffResistance" />
							<Label id="DebuffResistanceLabel" class="WindowText InventoryAttributeValue" text="0%" />
						</Panel>
						<Panel id="CriticalDamage" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_CriticalDamage', '#DOTA_Tooltip_CriticalDamage_Description')" onmouseout="DOTAHideTitleTextTooltip()">
