<root>
	<styles>
		<include src="file://{resources}/styles/windows/basic_window.css" />
		<include src="file://{resources}/styles/windows/item_tooltip.css" />
		<include src="file://{resources}/styles/windows/inventory_window.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/windows/inventory.js" />
		<include src="file://{resources}/scripts/windows/basic_window.js" />
	</scripts>
	<Panel class="Root" hittest="false">
		<Panel id="MainWindow" class="Window" style="width: 854px; height: 770px; visibility: collapse;">
