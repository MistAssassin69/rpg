							<Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CriticalDamage" />
							<Label id="CriticalDamageLabel" class="WindowText InventoryAttributeValue" text="100%" />
						</Panel>
						<Panel id="CriticalChance" class="InventoryAttributeWithValue" onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_CriticalChance', '#DOTA_Tooltip_CriticalChance_Description')" onmouseout="DOTAHideTitleTextTooltip()">
