<root>
    <styles>
        <include src="file://{resources}/styles/windows/basic_window.css"/>
        <include src="file://{resources}/styles/windows/talenttree_window.css"/>
        <include src="file://{resources}/styles/tooltips/custom/tooltip_item.css"/>
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/windows/basic_window.js"/>
        <include src="file://{resources}/scripts/windows/talenttree.js"/>
    </scripts>
    <Panel class="Root" hittest="false">
        <Panel id="MainWindow" class="Window TalentTreeWindow" style="width: 900px; height: 640px;">
            <Panel id="TalentTree" class="TalentTree">
            </Panel>
            <TextButton class="WindowCloseIcon" text="X" onactivate="CloseWindow()" />
            <Label id="TotalTalentPointsLabel" html="true" class="TotalTalentPointsLabel" text="#DOTA_TalentTree_Total_Talent_Points"/>
            <Label id="TalentTreeHelp" html="true" class="TalentTreeHelp" text="#DOTA_TalentTree_Help"/>
        </Panel>
        <Panel id="ItemTooltip" class="ItemTooltip" style="visibility: collapse;" hittest="false" hittestchildren="false">
            <Panel class="ItemTooltipHeader">
                <Panel>
                    <Image id="ItemTooltipImage" style="width: 64px; height: 64px;" class="ItemTooltipImage"
                           hittest="false"
                           scaling="stretch-to-fit-preserve-aspect"
                           src="file://{images}/spellicons/drow_ranger_wave_of_silence.png"/>
                </Panel>
                <Panel class="ItemTooltipName">
                    <Label id="ItemTooltipNameLabel" html="true" class="ItemTooltipNameLabel" text="Talent Name"/>
                </Panel>
            </Panel>
            <Panel class="ItemTooltipDescription">
                <Label id="ItemTooltipLabel" class="ItemTooltipLabel" html="true" text="Talent Description"/>
            </Panel>
        </Panel>

