<root>
    <styles>
        <include src="file://{resources}/styles/windows/basic_window.css"/>
        <include src="file://{resources}/styles/windows/saveload_window.css"/>
        <include src="file://{resources}/styles/windows/inventory_window.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/windows/basic_window.js"/>
        <include src="file://{resources}/scripts/windows/saveload.js"/>
    </scripts>
    <Panel class="Root" hittest="false">
        <Panel id="MainWindow" class="Window SaveLoadWindow">
            <Label class="SaveLoadTitleLabel" text="Select slot to save" />
            <Panel id="SlotsContainer">
                <Panel class="SaveSlot">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(0)" onmouseout="HideHeroInfo(0)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
                <Panel class="SaveSlot">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(1)" onmouseout="HideHeroInfo(1)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
                <Panel class="SaveSlot Empty">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(2)" onmouseout="HideHeroInfo(2)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
                <Panel class="SaveSlot Locked">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(3)" onmouseout="HideHeroInfo(3)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
                <Panel class="SaveSlot Locked">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(4)" onmouseout="HideHeroInfo(4)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
                <Panel class="SaveSlot Locked">
                    <Panel class="SaveSlotLeftContent">
                        <DOTAHeroImage id="SaveSlotHeroIcon" class="SaveSlotHeroIcon" heroname="npc_dota_hero_crystal_maiden" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect"/>
                        <Panel class="SaveSlotLeftContentText">
                            <Label class="SaveSlotLevelLabel" text="Level 0" />
                            <Label class="SaveSlotDateLabel" text="14:07, 27.05.2020" />
                        </Panel>
                    </Panel>
                    <Image onmouseover="ShowHeroInfo(5)" onmouseout="HideHeroInfo(5)" class="SaveSlotInfoIcon" src="file://{images}/custom_game/hud/inventory.png" scaling="stretch-to-fit-preserve-aspect" />
                    <Panel class="SaveSlotLockedIcon"/>
                </Panel>
            </Panel>
            <Image class="WindowCloseIcon" src="file://{images}/custom_game/hud/close_button.png"
                   scaling="stretch-to-fit-preserve-aspect" onactivate="CloseWindow()"/>
        </Panel>
    <Panel id="SaveSlotHeroPreview">
        <Panel class="HeroItems">
            <Panel id="HeroModelContainer">
                <DOTAScenePanel renderdeferred="false" class="HeroModel OverviewHeroRender" unit="npc_dota_hero_drow_ranger" drawbackground="1" allowrotation="false" antialias="false" activity-modifier="PostGameIdle" particleonly="false"/>
            </Panel>
            <Panel id="LeftItemSlots" class="LeftItemSlots">
                <Panel id="InventoryEquippedSlot0" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(0)" onmouseover="ShowEquippedItemTooltip(0)" onmouseout="HideItemTooltip()" showtooltip="false">
                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage0" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_claymore"/>
                </Panel>
                <Panel id="InventoryEquippedSlot1" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(1)" onmouseover="ShowEquippedItemTooltip(1)" onmouseout="HideItemTooltip()" showtooltip="false">
                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage1" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_chainmail"/>
                </Panel>
                <Panel id="InventoryEquippedSlot2" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(2)" onmouseover="ShowEquippedItemTooltip(2)" onmouseout="HideItemTooltip()" showtooltip="false">
                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage2" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_inventory_legs_background"/>
                </Panel>
                <Panel id="InventoryEquippedSlot3" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(3)" onmouseover="ShowEquippedItemTooltip(3)" onmouseout="HideItemTooltip()" showtooltip="false">
                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage3" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_boots"/>
                </Panel>
            </Panel>
            <Panel id="RightItemSlots" class="RightItemSlots">
                <Panel id="InventoryEquippedSlot5" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(5)" onmouseover="ShowEquippedItemTooltip(5)" onmouseout="HideItemTooltip()" showtooltip="false">
                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage5" scaling="stretch-to-fit-x-preserve-aspect" itemname="item_poor_mans_shield"/>
                </Panel>
                <Panel id="InventoryEquippedSlot6" class="InventoryEquippedSlot empty" draggable="true" oncontextmenu="OnRightClickOnInventoryEquippedSlot(6)" onmouseover="ShowEquippedItemTooltip(6)" onmouseout="HideItemTooltip()" showtooltip="false">
